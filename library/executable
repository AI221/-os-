shell.setAlias("a","/library/osStuff")
--force shell.programs to include our aliases
local a = shell.programs
function shell.programs(...)
  local b = a(...)
  b[#b+1] = "lock"
  b[#b+1] = "oldEdit"
  b[#b+1] = "startx"
  b[#b+1] = "console"
  for i=1,#prs do 
    b[#b+1] = prs[i]
  end 
  return b
end
